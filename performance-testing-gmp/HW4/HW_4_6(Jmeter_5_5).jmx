                <stringProp name="cacheKey">false</stringProp>
                <stringProp name="script">def allUsers = vars.get(&quot;ListOfUsersByMasc_ALL&quot;)
//def numUsers = vars.get(&quot;ListOfUsersByMasc_matchNr&quot;) as int
def newUserName = &quot;User_&quot;
//log.info(&quot;numUsers+++++++++++++++++++++&quot;+numUsers)

for (int i = 0; i &lt;= 99; i++) {
	if (i &lt; 10) {
		if (!allUsers.contains(&quot;User_0&quot; + i)) {
		newUserName = (newUserName + &quot;,User_0&quot; + i)
		log.info(&quot;User=i &lt; 10====================&quot;+newUserName)
	    }
	} else {
		if (!allUsers.contains(&quot;User_&quot; + i)) {
		newUserName = (newUserName + &quot;,User_&quot; + i)
		log.info(&quot;User=i &gt;= 10===================&quot;+newUserName)
		}
}
}
vars.put(&quot;newUserName&quot;, newUserName)
log.info(&quot;Final_newUserName===================&quot;+newUserName)

